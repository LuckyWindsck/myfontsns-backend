{
  "name": "myfontsns-backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/LuckyWindsck/myfontsns-backend.git",
  "author": "LuckyWind_sck <luckywindsck@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only .",
    "build": "tsc && yarn build:migrations && yarn build:seeders",
    "build:migrations": "rm -rf ./database/compiled/migrations && tsc --outDir ./database/compiled/migrations ./database/migrations/*.ts",
    "build:seeders": "rm -rf ./database/compiled/seeders ./database/compiled/factories && tsc --outDir ./database/compiled --resolveJsonModule ./database/seeders/*.ts",
    "lint": "eslint --ext=ts src tests bin",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "console:db": "node --experimental-repl-await --require ts-node/register/transpile-only ./bin/console-db.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.ts": "yarn lint:fix"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.1",
    "faker": "^5.5.3",
    "http-status-codes": "^2.1.4",
    "jsonapi-typescript": "^0.1.3",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.23",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "eslint": "^5.16.0 || ^6.8.0 || ^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
